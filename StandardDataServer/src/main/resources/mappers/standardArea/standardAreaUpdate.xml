<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- resources/mapper/StandardAreaInsertMapper.xml -->
<mapper namespace="com.dms.standarddataserver.standardArea.mapper.StandardAreaUpdateMapper">
    <update id="update" parameterType="com.dms.standarddataserver.standardArea.dto.StandardAreaDTO">
        UPDATE subj_area
        SET subj_area_NM = #{standardAreaDTO.stdAreaNm},
        subj_area_DESC = #{standardAreaDTO.stdAreaDesc}
        WHERE subj_area_NM = #{standardAreaDTO.message}
    </update>
</mapper>
